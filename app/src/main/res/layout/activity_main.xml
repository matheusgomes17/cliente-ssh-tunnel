			android:textAppearance="?android:attr/textAppearanceSmall"
			android:gravity="center"
			android:textStyle="bold"
			android:textColor="#fffcfcfc"
			android:layout_gravity="center"/>

		<LinearLayout
			android:orientation="vertical"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent"
			android:layout_marginStart="12.0dip"
			android:layout_marginEnd="12.0dip"
			android:background="@drawable/nv1">

			<TextView
				android:layout_width="110dp"
				android:layout_height="110dp"
				android:textAppearance="?android:attr/textAppearanceLarge"
				android:background="@drawable/logox"
				android:gravity="center"
				android:layout_gravity="center"
				android:textSize="0sp"
				android:layout_margin="10dp"
				android:layout_marginTop="4dp"/>

			<TextView
				android:layout_width="280dp"
				android:layout_height="wrap_content"
				android:text="𝐐𝐮𝐞𝐦 𝐦𝐞 𝐝𝐞𝐫𝐚 𝐞𝐮 𝐩𝐨𝐝𝐞𝐫 𝐯𝐨𝐚𝐫 𝐞𝐮 𝐣𝐮𝐫𝐨 𝐪𝐮𝐞 𝐥𝐞𝐯𝐚𝐯𝐚 𝐯𝐨𝐜̧𝐞 🖤"
				android:textAppearance="?android:attr/textAppearanceLarge"
				android:textSize="19sp"
				android:layout_gravity="center"
				android:background="@drawable/bg1"
				android:gravity="center"/>

			<LinearLayout
				android:orientation="vertical"
				android:layout_width="fill_parent"
				android:layout_height="fill_parent"
				android:layout_marginStart="12.0dip"
				android:layout_marginEnd="12.0dip"
				android:background="@drawable/nv1">

				<LinearLayout
					android:layout_gravity="right"
					android:orientation="horizontal"
					android:layout_width="100.0dip"
					android:background="@drawable/mini"
					android:layout_height="20.0dip"
					android:layout_marginTop="10.0dip"
					android:layout_marginEnd="10dp">

					<LinearLayout
						android:orientation="horizontal"
						android:layout_width="16.0dip"
						android:layout_height="16.0dip"
						android:background="@drawable/image_8"
						android:layout_marginLeft="10dp"
						android:gravity="center"
						android:layout_gravity="center"/>

					<TextView
						android:textAppearance="?android:textAppearanceSmall"
						android:textSize="12.0sp"
						android:background="@drawable/mini1"
						android:textColor="#fffcfcfc"
						android:layout_width="fill_parent"
						android:layout_height="fill_parent"
						android:text="Servidor"
						android:layout_marginStart="4.0dip"
						android:gravity="center"/>

				</LinearLayout>

				<Spinner

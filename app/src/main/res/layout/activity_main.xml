						android:background="@drawable/bg1"
						android:layout_height="45.0dip"
						android:textColor="#fffcfcfc"
						android:spinnerMode="dropdown"
						android:prompt="@string/category_prompt"
						android:id="@+id/serverSpinner" />

				<Spinner

						android:layout_marginRight="5dp"
						android:textColor="#fffcfcfc"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:prompt="@string/category_prompt"
						android:id="@+id/proxyxx" tools:ignore="DuplicateIds"/>

				<LinearLayout
					android:layout_gravity="right"
					android:orientation="horizontal"
					android:layout_width="100.0dip"
					android:background="@drawable/mini"
					android:layout_height="20.0dip"
					android:layout_marginTop="5.0dip"
					android:layout_marginEnd="10dp">

					<LinearLayout
						android:orientation="horizontal"
						android:layout_width="16.0dip"
						android:layout_height="16.0dip"
						android:layout_marginLeft="10dp"
						android:gravity="center"
						android:layout_gravity="center"
						android:background="@drawable/image_9"/>

					<TextView
						android:textAppearance="?android:textAppearanceSmall"
						android:textSize="12.0sp"
						android:background="@drawable/mini1"
						android:textColor="#fffcfcfc"
						android:layout_width="fill_parent"
						android:layout_height="fill_parent"
						android:text="payload"
						android:layout_marginStart="4.0dip"
						android:gravity="center"/>

				</LinearLayout>

				<Spinner
					style="@style/SpinnerTheme"
					android:layout_width="fill_parent"
					android:popupBackground="@drawable/bg"
					android:background="@drawable/bg1"
					android:layout_height="45.0dip"
					android:textColor="#fffcfcfc"
					android:spinnerMode="dropdown"
					android:id="@+id/payloadSpinner"/>

				<RelativeLayout
					android:layout_width="360dp"
					android:layout_height="wrap_content"
					android:layout_gravity="center"
					android:layout_marginTop="5dp">

					<RelativeLayout
						android:layout_width="180dp"
						android:layout_height="45dp"
						android:background="@drawable/bg1"
						android:orientation="vertical"
						android:layout_marginBottom="5dp">

						<RelativeLayout
								android:layout_width="match_parent"
								android:layout_height="45dp"
								android:background="@drawable/h_spin_shp1"
								android:id="@+id/h_u_l2"
								android:layout_margin="5dp" tools:ignore="DuplicateIds">

							<com.google.android.material.textfield.TextInputEditText
									android:id="@+id/activity_mainInputPasswordUserEdit"
									android:background="@null"
									android:layout_width="fill_parent"
									android:layout_height="match_parent"
									android:hint="Usuario"
									android:imeOptions="actionUnspecified|normal"
									android:nextFocusDown="@+id/HarliePasss"
									android:singleLine="true"
									android:inputType="text"
									android:textSize="14sp"
									android:layout_alignParentRight="true"
									android:layout_marginLeft="40dp"
									android:textColorHint="#000000"
									android:paddingLeft="5dp" tools:ignore="DuplicateIds"
									android:layout_centerInParent="true"/>

							<ImageView
								android:tint="@color/h_c"
								android:layout_width="29dp"
								android:layout_height="29dp"
								android:src="@drawable/h_acnt"
								android:layout_margin="10dp"/>

						</RelativeLayout>

						<TextView
							android:visibility="gone"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:textStyle="italic"
							android:text="Usuario"
							android:layout_marginLeft="25dp"
							android:background="@drawable/ic_gaming"
							android:textColor="#000000"
							android:textSize="10sp"
							android:padding="2dp"
							android:layout_marginTop="-4dp"/>

					</RelativeLayout>

					<RelativeLayout
						android:layout_width="190dp"
						android:background="@drawable/bg1"
						android:layout_height="45dp"
						android:layout_marginLeft="190dp">

						<RelativeLayout
							android:layout_width="match_parent"
							android:layout_height="45dp"
							android:background="@drawable/h_spin_shp1"
							android:id="@+id/fragment_userpassTextInputLayout2"
							android:gravity="center"
							android:layout_margin="5dp"
							android:layout_marginTop="5dp">

							<com.google.android.material.textfield.TextInputEditText
								android:id="@+id/activity_mainInputPasswordPassEdit"
								android:background="@null"
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:hint="Senha"
								android:singleLine="true"
								android:textSize="14sp"
								android:inputType="textPassword"
								android:layout_centerInParent="true"
								android:layout_alignParentLeft="true"
								android:layout_marginLeft="40dp"
								android:textColorHint="#000000"
								android:paddingLeft="5dp"/>

							<ImageView
								android:tint="@color/h_c"
								android:layout_width="29dp"
								android:layout_height="29dp"
								android:src="@drawable/h_pass"
								android:layout_margin="10dp"/>

							<ImageButton
								android:padding="10dp"
								android:background="@android:color/transparent"
								android:id="@+id/show_password"
								android:src="@drawable/ic_visibility_black_24dp"
								android:layout_width="wrap_content"
								android:layout_height="match_parent"
								android:hint="@string/show_password"
								android:layout_gravity="right"/>

						</RelativeLayout>

						<TextView
							android:visibility="gone"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:textStyle="italic"
							android:text="Senha"
							android:layout_marginLeft="25dp"
							android:background="@drawable/ic_gaming"
							android:textColor="#000000"
							android:textSize="10sp"
							android:padding="2dp"
							android:layout_marginTop="-4dp"/>

					</RelativeLayout>

				</RelativeLayout>

				<LinearLayout
					android:orientation="horizontal"
					android:layout_width="180dp"
					android:background="@drawable/borda1"
					android:layout_height="30.0dip"
